{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_document.tsx",
    "pages/index.tsx",
    "pages/_app.tsx"
, "tsconfig.json", "utils/.ImageTools.ts", "components/.MapMarker", "mixins/facebook.js", "pages/api/auth/signin.jsx", "pages/api/auth/[...nextauth].Js"  ],
  "exclude": [
    "node_modules"
  ]
}